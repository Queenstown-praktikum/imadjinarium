{
  "name": "imadjinarium",
  "version": "1.1.0",
  "description": "Game Imadjinarium, training project yandex practicum",
  "main": "index.tsx",
  "scripts": {
    "start": "webpack-dev-server --config ./webpackConfig/webpack.dev.conf.js",
    "start:server": "tsc --project server && nodemon ./server-dist/server/index.js",
    "build:client": "webpack --config ./webpackConfig/webpack.prod.conf.js",
    "build:client:ssr": "webpack --config ./webpackConfig/ssr.conf.js --watch",
    "build:server": "tsc --project server",
    "lint": "eslint client/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix 'client/**/*.{js,jsx,ts,tsx}'",
    "stylelint": "stylelint src/**/*.{css,scss}",
    "stylelint:fix": "stylelint --fix client/**/*.{css,scss}",
    "format": "prettier --write 'client/**/*.{js,jsx,ts,tsx,css,md,json,less,sass}' --config ./.prettierrc",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "server": "npm run build && node ./server/server.ts",
    "test": "jest"
  },
  "lint-staged": {
    "client/**/*.(js|jsx|ts|tsx)": "eslint --fix",
    "client/**/*.(css|sass|scss)": "stylelint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Queenstown-praktikum/imadjinarium.git"
  },
