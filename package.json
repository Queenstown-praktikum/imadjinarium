    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.1",
    "css-modules-typescript-loader": "4.0.1",
    "eslint": "8.19.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.2.5",
    "eslint-plugin-import": "2.26.0",
