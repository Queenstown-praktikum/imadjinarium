{
  "name": "imadjinarium",
  "version": "1.0.0",
  "description": "Game Imadjinarium, training project yandex practicum",
  "main": "index.tsx",
  "scripts": {
    "start:dev": "webpack-dev-server --config ./webpackConfig/webpack.dev.conf.js",
    "start:build": "webpack --config ./webpackConfig/webpack.prod.conf.js",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "stylelint": "stylelint src/**/*.{css,scss}",
    "stylelint:fix": "stylelint --fix src/**/*.{css,scss}",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json,less,sass}' --config ./.prettierrc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.(js|jsx|ts|tsx)": "eslint --fix",
    "src/**/*.(css|sass|scss)": "stylelint --fix"
  },
  "repository": {
